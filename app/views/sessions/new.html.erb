  <%= form_for(:session, url: sessions_path) do |f| %>
    <div class="field">      
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <br>
    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <br>
    <%= f.submit "GO IN MONGO!", class: "btn btn-large btn-default" %>
  <% end %>
